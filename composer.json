{
    "name": "motomedialab/stub",
    "type": "library",
    "description": "Helper package to stub out a MotoMediaLab project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Motomedialab\\Stub\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Motomedialab\\Stub\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-root-package-install": "@php artisan motomedialab:stub"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motomedialab\\Stub\\Providers\\StubServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
